   "execution_count": 9,
   "id": "2fdf4b21",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-08-05T09:31:45.118362Z",
     "iopub.status.busy": "2024-08-05T09:31:45.118039Z",
